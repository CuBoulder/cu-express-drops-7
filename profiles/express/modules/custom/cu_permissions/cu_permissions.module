/**
 * Implements hook_menu_alter.
 * Alter menus that do not restrict access to users with 'administer site configuration' 
 */
function cu_permissions_menu_alter(&$items) {
  // http://cgit.drupalcode.org/menu_block/tree/menu_block.module#n69
  $items['admin/config/user-interface/menu-block']['access callback'] = 'user_access';
  $items['admin/config/user-interface/menu-block']['access arguments'] = array('administer site configuration');
}

